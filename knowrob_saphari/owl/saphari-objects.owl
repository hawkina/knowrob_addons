<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE rdf:RDF [ 
  <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
  <!ENTITY srdl2 "http://knowrob.org/kb/srdl2.owl#" >
  <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
  <!ENTITY knowrob "http://knowrob.org/kb/knowrob.owl#" >
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
  <!ENTITY srdl2-comp "http://knowrob.org/kb/srdl2-comp.owl#" >
  <!ENTITY srdl2-cap "http://knowrob.org/kb/srdl2-cap.owl#" >
  <!ENTITY qudt-unit "http://qudt.org/vocab/unit#" >
  <!ENTITY objects "http://knowrob.org/kb/saphari-objects.owl#" >
]>


<rdf:RDF xmlns="http://knowrob.org/kb/saphari-objects.owl#"
  xml:base="http://knowrob.org/kb/saphari-objects.owl"
  xmlns:knowrob="http://knowrob.org/kb/knowrob.owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
  xmlns:srdl2="http://knowrob.org/kb/srdl2.owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:srdl2-cap="http://knowrob.org/kb/srdl2-cap.owl#"
  xmlns:srdl2-comp="http://knowrob.org/kb/srdl2-comp.owl#"
  xmlns:qudt-unit="http://qudt.org/vocab/unit#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

    <owl:Ontology rdf:about="http://knowrob.org/kb/saphari-objects.owl">
        <owl:imports rdf:resource="package://knowrob_common/owl/knowrob.owl"/>
    </owl:Ontology>

    <owl:Class rdf:about="&knowrob;Bowl">
        <rdfs:subClassOf rdf:resource="&knowrob;Bowl-Generic"/>
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <!-- =========================================== -->
    <!-- |   Tools                                 | -->
    <!-- =========================================== -->

    <owl:Class rdf:about="&knowrob;SurgeryTool">
        <rdfs:subClassOf rdf:resource="&knowrob;HandTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;Scissors">
        <rdfs:subClassOf rdf:resource="&knowrob;CuttingDevice"/>
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;Scalpel">
        <rdfs:subClassOf rdf:resource="&knowrob;CuttingDevice"/>
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;ScalpelBlade">
        <rdfs:subClassOf rdf:resource="&knowrob;CuttingDevice"/>
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;Rake">
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;ClampBig">
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;ClampSmall">
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;Pincers">
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>

    <owl:Class rdf:about="&knowrob;Hook">
        <rdfs:subClassOf rdf:resource="&knowrob;SurgeryTool"/>
    </owl:Class>
</rdf:RDF>
